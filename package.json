        "prop-types": "^15.6.2",
        "react": "^16.6.1",
        "react-dom": "^16.6.1",
        "react-intl": "^2.7.2",
        "react-loader": "^2.4.5",
        "react-redux": "^5.1.0",
        "react-router-dom": "^4.3.1",
